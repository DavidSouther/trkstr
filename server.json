{
  "stassets": {
    "verbose": true,
    "root": "./src/client",
    "scripts": {
      "types": [
        "store", "actions", "component"
      ]
    },
    "vendors": {
      "prefix": ["./src/vendors", "./node_modules"],
      "js": [
        "EventEmitter.js",
        "song-dispatcher/song-dispatcher.js",
        "immutable.min.js"
      ],
      "css": []
    },
    "material": {
      "themes": ["indigo"]
    }
  },
  "tls": false,
  "routing": [
    "./src/server/allow.coffee",
    "./src/server/**/*route.coffee"
  ],
  "static": {
    "img": "./src/assets/img",
    "/": "./music"
  }
}
